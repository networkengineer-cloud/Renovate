{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["images.yaml"],
      "matchStrings": [
        "^(?<depName>[\\w-]+/[\\w-]+):(?<currentValue>\\d+\\.\\d+\\.\\d+)$"
      ],
      "datasourceTemplate": "docker",
      "versioningTemplate": "docker"
    }
  ],
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "versioning": "regex:^(\\d+\\.\\d+\\.\\d+)$"
    }
  ]
}

